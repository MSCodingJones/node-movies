<%- include('../partials/header'); %>

<main class="main" id="allMain">
    <div class="container">
        <div class="row row-cols-1 row-cols-md-4 g-4">
            <% data.forEach(item => { %>
                <div class="col">
                    <div class="card">
                        <img src="<%= item.url %>" alt="image of">
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</main>
